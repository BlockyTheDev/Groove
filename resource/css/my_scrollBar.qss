QScrollBar#minScrollBar {
    background: transparent;
    width: 3px;
}

/*隐藏上箭头*/
QScrollBarr#minScrollBar::sub-line {
    background: transparent;
}

/*隐藏下箭头*/
QScrollBar#minScrollBar::add-line {
    background: transparent;
}

QScrollBar#minScrollBar::handle {
    background: rgb(122, 122, 122);
    border: 0px solid transparent;
    min-height: 40px;
}
