QWidget[state='leave']{
    background-color: transparent;
}

QWidget[state='hover'] {
    background-color: rgb(41, 50, 54);
}

QWidget[state='pressed'] {
    background-color: rgb(86,77,77);
}


QToolButton {
    background-color: transparent;
    border: none;
    margin: 0
}

QToolButton:hover{
    background: rgb(62, 61, 60);
}

QToolButton:pressed{
    background-color: rgb(82, 89, 93);
}

